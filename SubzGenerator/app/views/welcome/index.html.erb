<!--Intro and service-->
<div id="intro" class="section scrollspy">
    <div class="container">
        <div class="row">
            <div  class="col s12">
                  <h2 class="center header text_h2"> Just upload your movie or video clip <span class="span_h2"> Easy as that </span>, no extra download no headeags <span class="span_h2">, on the go.</span> </h2>
            </div>

            <div  class="col s12 m4 l4">
                <div class="center promo promo-example">
                  <%= image_tag("/assets/generate.png",  alt: "", class: "mdi-image-flash-on", size: "175x175") %>
                  <h5 class="promo-caption">Realtime generating.</h5>
                  <p class="light center">Generating a srt.file with your subtitles.</p>
                </div>
            </div>
            <div class="col s12 m4 l4">
                <div class="center promo promo-example">
                  <%= image_tag("/assets/lazy.jpg",  alt: "", class: "mdi-image-flash-on", size: "175x175") %>
                  <h5 class="promo-caption">No extra work</h5>
                  <p class="light center">Auto synchronizing with your movie just sitback and relax</p>
                </div>
            </div>
            <div class="col s12 m4 l4">
                <div class="center promo promo-example">
                  <%= image_tag("/assets/responsive.jpg",  alt: "", class: "mdi-image-flash-on", size: "250x175") %>
                  <h5 class="promo-caption">Fully responsive</h5>
                  <p class="light center">Satisfying your needs</p>
                </div>
            </div>
        </div>
    </div>
</div>
